{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "extends": [
    "plugin:n8n-nodes-base/community"
  ],
  "ignorePatterns": [
    "node_modules/**",
    "dist/**",
    "test/**",
    "*.js",
    "*.d.ts"
  ],
  "rules": {
    "@typescript-eslint/no-unused-vars": "off",
    "n8n-nodes-base/node-dirname-against-convention": "off"
  }
}
